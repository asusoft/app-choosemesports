{
  "name": "admin-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:validate": "previous_hash=$(npm run --silent codegen:hash) && npm run codegen && if test $previous_hash = $(npm run --silent codegen:hash);then echo 'Types is consistent within current schema'; exit 0; else echo 'Types is not consistent within schema'; exit 1; fi",
    "codegen:hash": "echo $(find ./src/shared/generated -type f -print0 | sort -z | xargs -0 sha256sum | sha256sum | cut -d \" \" -f 1);"
  },
  "dependencies": {
    "@apollo/client": "^3.9.11",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.5",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@mui/system": "^5.15.15",
    "@types/apollo-upload-client": "^18.0.0",
    "@types/jest": "^29.5.12",
    "@types/react-test-renderer": "^18.0.7",
    "apollo-upload-client": "^18.0.1",
    "babel-jest": "^29.7.0",
    "graphql": "^16.8.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.22.3",
    "react-router-dom": "^6.22.3",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "vite": "^5.2.0"
  }
}
