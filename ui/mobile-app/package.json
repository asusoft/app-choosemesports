{
  "name": "choosemesports",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache $ESLINT_ARG",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --cache",
    "lint:ts": "tsc --noEmit",
    "lint:ci": "npm run lint && npm run codegen:validate && npm run spec:validate && npm run lint:ts",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:validate": "previous_hash=$(npm run --silent codegen:hash) && npm run codegen && if test $previous_hash = $(npm run --silent codegen:hash);then echo 'Types is consistent within current schema'; exit 0; else echo 'Types is not consistent within schema'; exit 1; fi",
    "codegen:hash": "echo $(find ./src/shared/generated -type f -print0 | sort -z | xargs -0 sha256sum | sha256sum | cut -d \" \" -f 1);"
  },
  "dependencies": {
    "@apollo/client": "^3.9.4",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.2",
    "@graphql-codegen/typescript": "^4.0.4",
    "@graphql-codegen/typescript-operations": "^4.1.2",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@react-native-async-storage/async-storage": "^1.22.3",
    "@react-native-community/blur": "^4.4.0",
    "@react-native-community/slider": "^4.5.0",
    "@react-navigation/bottom-tabs": "^6.5.16",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/native-stack": "^6.9.18",
    "@react-navigation/stack": "^6.3.21",
    "@types/react-native-video": "^5.0.19",
    "apollo-upload-client": "^17.0.0",
    "graphql": "^16.8.1",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-country-flag": "^2.0.2",
    "react-native-date-picker": "^5.0.2",
    "react-native-document-picker": "^9.1.1",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-image-crop-picker": "^0.40.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-notifier": "^1.9.0",
    "react-native-paper": "^5.12.3",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-svg": "^14.1.0",
    "react-native-svg-transformer": "^1.3.0",
    "react-native-swipe-modal-up-down": "^1.1.0",
    "react-native-track-player": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@tsconfig/react-native": "^3.0.3",
    "@types/apollo-upload-client": "^17.0.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.55",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
